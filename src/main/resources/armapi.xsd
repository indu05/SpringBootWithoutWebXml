    <xs:element name="synchUserRequest">
        <xs:complexType>
            <xs:sequence>
               <xs:element name="name" type="xs:string"/> 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="synchUserResponse">
